{
  "name": "idn-area",
  "version": "4.3.0",
  "description": "API that provides information about Indonesia administrative area.",
  "author": "fityannugroho <fityannugroho@gmail.com> (https://github.com/fityannugroho)",
  "funding": "https://github.com/sponsors/fityannugroho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fityannugroho/idn-area"
  },
  "keywords": [
    "idn-area",
    "indonesia",
    "wilayah",
    "area",
    "data",
    "api"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:gen": "tsx prisma/generate.ts",
    "db:migrate": "tsx prisma/migrate.ts",
    "db:seed": "prisma db seed",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run --no-threads",
    "test:watch": "vitest --no-threads",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/static": "^7.0.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@prisma/client": "^5.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@swc/core": "^1.4.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "idn-area-data": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "vite": "^4.5.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
